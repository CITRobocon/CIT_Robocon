#include "mbed.h"
#include "AutoMachine.h"

Serial PC(USBTX, USBRX);

int main (void){
    while(1){
         PC.printf ("                %f  %f\n%f                              %f\n%f                              %f\n                %f  %f\n\r"
                    , FL.read(), FR.read(), LF.read(), RF.read(), LB.read(), RB.read(), BL.read(), BR.read());
    }
}
